		<servlet-class>br.com.servlets.Passo3</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>Passo1</servlet-name>
		<url-pattern>*.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Passo2</servlet-name>
		<url-pattern>/fooStuf/bar</url-pattern>
	</servlet-mapping>
	
	<!-- 
	<servlet-mapping>
		<servlet-name>Passo3</servlet-name>
		<url-pattern>/fooStuf/*</url-pattern>
	</servlet-mapping>
